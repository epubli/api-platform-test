{
    "name": "l0wskilled/api-platform-test",
    "description": "Bundle to ease the test of Endpoints generated by ApiPlatform.",
    "license": "MIT",
    "authors": [
        {
            "name": "Hendrik Knigge",
            "email": "h.knigge@epubli.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "symfony/browser-kit": "^4.3",
        "fzaninotto/faker": "^1.9",
        "doctrine/orm": "^2.6",
        "symfony/framework-bundle": "^4.3",
        "symfony/validator": "^4.3",
        "symfony/phpunit-bridge": ">=4.3"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "to use the ORM extensions",
        "doctrine/mongodb-odm-bundle": "to use the MongoDB ODM extensions"
    },
    "autoload": {
        "psr-4": {
            "Epubli\\ApiPlatform\\TestBundle\\": "src/"
        }
    }
}
